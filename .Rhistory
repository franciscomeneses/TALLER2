data <- iccssimce %>% select("p.leng"=ptje_lect8b_alu, "p.mat"=ptje_mate8b_alu, "p.nat"=ptje_nat8b_alu, "p.civ"=PV1CIV, "nse"=S_NISB, "int_pol_es"=S_SINT, "ip_padres"=S_HINT,idschool, "cult_part"=S_VALPARTS) %>% as.data.frame()
data <- data %>%
